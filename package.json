{
  "name": "otto-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "download-schema": "apollo service:download graphql-schema.json",
    "codegen": "yarn download-schema && apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --tagName=gql --globalTypesFile='./src/__generated__/global-types.ts' && typechain --target=ethers-v5 ./src/contracts/abis/**/*.json --out-dir=./src/contracts/__generated__/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@reduxjs/toolkit": "^1.8.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@usedapp/core": "^1.0.0",
    "@web3-react/walletconnect-connector": "^6.2.10",
    "@web3-react/walletlink-connector": "^6.2.13",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "graphql": "^15.0.0",
    "i18next": "^21.6.14",
    "i18next-browser-languagedetector": "^6.1.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.7",
    "react-markdown": "^8.0.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.3",
    "typechain": "^8.0.0"
  },
  "devDependencies": {
    "@types/graphql": "14.5.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "apollo": "2.33.9",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "react-error-overlay": "6.0.9",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "graphql": "^15.0.0"
  }
}
